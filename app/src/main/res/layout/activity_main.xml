<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <LinearLayout
        android:id="@+id/first_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp">

        <ImageView
            android:id="@+id/first_entry_pic"
            android:layout_width="93dp"
            android:layout_height="91dp"
            android:src="@drawable/two"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="49dp" />

        <TextView
            android:id="@+id/first_entry_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="22232284" />

        <TextView
            android:id="@+id/first_entry_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Kenneth James Macas" />

        <TextView
            android:id="@+id/first_entry_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="09351599675" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/second_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/second_entry_pic"
            android:layout_width="93dp"
            android:layout_height="91dp"
            android:src="@drawable/three"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="195dp" />

        <TextView
            android:id="@+id/second_entry_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="22232791" />

        <TextView
            android:id="@+id/second_entry_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Jemuel Rosal" />

        <TextView
            android:id="@+id/second_entry_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="09551326770" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/third_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="50dp">

        <ImageView
            android:id="@+id/third_entry_pic"
            android:layout_width="93dp"
            android:layout_height="91dp"
            tools:layout_editor_absoluteX="31dp"
            android:src="@drawable/four"
            tools:layout_editor_absoluteY="358dp" />

        <TextView
            android:id="@+id/third_entry_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="22232284" />

        <TextView
            android:id="@+id/third_entry_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Alvin John Araneta" />

        <TextView
            android:id="@+id/third_entry_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="09972577724" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/fourth_entry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/fourth_entry_pic"
            android:layout_width="93dp"
            android:layout_height="91dp"
            android:src="@drawable/one"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="503dp" />

        <TextView
            android:id="@+id/fourth_entry_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="222232291" />

        <TextView
            android:id="@+id/fourth_entry_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Clint Dart Diamos" />

        <TextView
            android:id="@+id/fourth_entry_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="0936814321" />
    </LinearLayout>

</LinearLayout>